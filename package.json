    "eslint-plugin-vue": "7.9.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuicer/vuepress-theme-yuicer.git"
  },
