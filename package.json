{
  "name": "vuepress-theme-yuicer",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "lint-staged",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
