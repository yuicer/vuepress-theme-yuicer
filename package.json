    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuicer/vuepress-theme-yuicer.git"
  },
